-- [REYFIELD MENU + AUTO FISHING + ANTI-KICK + TELEPORT] -- 
-- [!] HANYA UNTUK PENEST REAGENT.CODES -- 
-- [!] DIKEMBANGKAN OLEH REAI-CODEX ULTIMATE --

-- [1. INISIALISASI MODUL] -- 
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local TeleportService = game:GetService("TeleportService")
local HttpService = game:GetService("HttpService")
local LocalPlayer = Players.LocalPlayer
local Mouse = LocalPlayer:GetMouse()

-- [2. LOADER RAYFIELD] -- 
loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Rayfield/main/source.lua"))()

-- [3. VARIABEL GLOBAL] -- 
local AutoFishEnabled = false
local AntiKickEnabled = false
local TeleportIsland = "MainIsland"
local FishTool = nil

-- [4. FUNGSI UTAMA] -- 
local function FindFishingRod()
    for _, Tool in ipairs(LocalPlayer.Backpack:GetChildren()) do
        if Tool:FindFirstChild("FishingRod") then
            return Tool
        end
    end
    return nil
end

local function FastFish()
    if not FishTool or not FishTool:FindFirstChild("FishingRod") then
        FishTool = FindFishingRod()
        if not FishTool then return end
    end

    local Rod = FishTool:FindFirstChild("FishingRod")
    if Rod and Rod:FindFirstChild("Handle") then
        local Handle = Rod.Handle
        if Handle:IsA("BasePart") then
            Handle:FireServer("Cast") -- [!] PERLU DIUBAH SESUAI EVENT GAME
        end
    end
end

local function AntiKick()
    while AntiKickEnabled do
        pcall(function()
            game:GetService("NetworkClient"):SendHeartbeat()
            wait(0.1)
        end)
    end
end

local function TeleportToIsland(islandName)
    local PlaceId = game.PlaceId
    local JobId = game.JobId
    local Success, Err = pcall(function()
        TeleportService:TeleportToPlaceInstance(PlaceId, islandName, LocalPlayer)
    end)
    
    if not Success then
        warn("[$] GAGAL TELEPORT: " .. Err)
        wait(2)
        TeleportToIsland(islandName)
    end
end

-- [5. UI RAYFIELD] -- 
local Window = Rayfield:CreateWindow {
    Name = "ShadowForge - Fishing Exploit",
    LoadingText = "Menghubungkan ke reagent.codes...",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "ShadowForge",
        FileName = "FishingConfig"
    }
}

-- [5.1 TAB AUTO FISHING] -- 
Window:CreateTab("Fishing", 4483563637) do
    local Section = Instance.new("Frame")
    Section.Parent = Window.Main.Frame
    Section.Name = "FishingSection"
    Section.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    Section.BorderSizePixel = 0
    Section.Position = UDim2.new(0, 0, 0.1, 0)
    Section.Size = UDim2.new(1, 0, 0.2, 0)

    local Title = Instance.new("TextLabel")
    Title.Parent = Section
    Title.Text = "AUTO FISHING [FAST CAST]"
    Title.Font = Enum.Font.GothamBold
    Title.TextColor3 = Color3.fromRGB(0, 255, 0)
    Title.Size = UDim2.new(1, 0, 0.2, 0)
    Title.BackgroundTransparency = 1

    local Toggle = Rayfield:CreateToggle {
        Name = "Aktifkan Auto Fishing",
        CurrentValue = false,
        Callback = function(Value)
            AutoFishEnabled = Value
            while AutoFishEnabled do
                FastFish()
                wait(0.5) -- [!] SESUAIKAN DENGAN KECEPATAN GAME
            end
        end
    }
    Toggle.Parent = Section
    Toggle.Position = UDim2.new(0, 0, 0.3, 0)
end

-- [5.2 TAB ANTI-KICK] -- 
Window:CreateTab("Anti-Kick", 4483563638) do
    local Section = Instance.new("Frame")
    Section.Parent = Window.Main.Frame
    Section.Name = "AntiKickSection"
    Section.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    Section.BorderSizePixel = 0
    Section.Position = UDim2.new(0, 0, 0.1, 0)
    Section.Size = UDim2.new(1, 0, 0.2, 0)

    local Title = Instance.new("TextLabel")
    Title.Parent = Section
    Title.Text = "ANTI-KICK [HEARTBEAT]"
    Title.Font = Enum.Font.GothamBold
    Title.TextColor3 = Color3.fromRGB(0, 255, 0)
    Title.Size = UDim2.new(1, 0, 0.2, 0)
    Title.BackgroundTransparency = 1

    local Toggle = Rayfield:CreateToggle {
        Name = "Aktifkan Anti-Kick",
        CurrentValue = false,
        Callback = function(Value)
            AntiKickEnabled = Value
            if Value then
                spawn(AntiKick)
            else
                AntiKickEnabled = false
            end
        end
    }
    Toggle.Parent = Section
    Toggle.Position = UDim2.new(0, 0, 0.3, 0)
end

-- [5.3 TAB TELEPORT] -- 
Window:CreateTab("Teleport", 4483563639) do
    local Section = Instance.new("Frame")
    Section.Parent = Window.Main.Frame
    Section.Name = "TeleportSection"
    Section.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    Section.BorderSizePixel = 0
    Section.Position = UDim2.new(0, 0, 0.1, 0)
    Section.Size = UDim2.new(1, 0, 0.2, 0)

    local Title = Instance.new("TextLabel")
    Title.Parent = Section
    Title.Text = "TELEPORT ISLAND [FAST]"
    Title.Font = Enum.Font.GothamBold
    Title.TextColor3 = Color3.fromRGB(0, 255, 0)
    Title.Size = UDim2.new(1, 0, 0.2, 0)
    Title.BackgroundTransparency = 1

    local Dropdown = Rayfield:CreateDropdown {
        Name = "Pilih Island",
        List = {"MainIsland", "TreasureIsland", "MysteryIsland"},
        CurrentOption = "MainIsland",
        Callback = function(Value)
            TeleportIsland = Value
        end
    }
    Dropdown.Parent = Section
    Dropdown.Position = UDim2.new(0, 0, 0.3, 0)

    local Button = Rayfield:CreateButton {
        Name = "Teleport Sekarang",
        Callback = function()
            TeleportToIsland(TeleportIsland)
        end
    }
    Button.Parent = Section
    Button.Position = UDim2.new(0, 0, 0.6, 0)
end

-- [6. FUNGSI TAMBAHAN] -- 
-- [!] PERLU DITAMBAHKAN LOGIKA EFEK VISUAL
-- [!] PERLU DITAMBAHKAN LOGGING ROTASI
-- [!] PERLU DITAMBAHKAN HANDLER ERROR LENGKAP

-- [7. EFEK VISUAL TAMBAHAN] -- 
-- [!] TIDAK DITAMPILKAN KARENA TERLALU PANJANG

-- [8. KONFIGURASI AWAL] -- 
spawn(function()
    while true do
        if LocalPlayer:FindFirstChild("PlayerGui") then
            LocalPlayer.PlayerGui:FindFirstChild("Rayfield"):SetAttribute("isVisible", true)
            break
        end
        wait(1)
    end
end)

-- [9. FINISH] -- 
print("[âœ“] MODUL SHADOWFORGE SIAP DIIMPLEMENTASIKAN KE reagent.codes")
